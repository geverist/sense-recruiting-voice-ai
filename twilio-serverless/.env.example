# Twilio Serverless Environment Variables
# Copy this to .env and fill in your values

# Twilio Account Credentials
ACCOUNT_SID=
AUTH_TOKEN=

# Twilio Resources
DEFAULT_TWILIO_NUMBER=
TWILIO_SYNC_SVC_SID=

# Twilio Conversational Intelligence (Optional but Recommended)
# Provides real-time transcription, PII redaction, sentiment analysis
# Create at: https://console.twilio.com/us1/develop/conversations/services
INTELLIGENCE_SERVICE_SID=

# OpenAI API (Required for AI-powered dynamic responses)
# Sign up at: https://platform.openai.com
# Get API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# ElevenLabs Voice API (Optional - for premium voices)
# Sign up at: https://elevenlabs.io
# Get API key from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=

# Your Main Application WebSocket URL
# This is where your Express app with the AI conversation logic runs
HOSTNAME=your-domain.ngrok-free.app

# Domain Name (auto-populated by Twilio Serverless)
DOMAIN_NAME=
